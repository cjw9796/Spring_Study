
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- MvcboardVO 클래스 bean -->
	<bean id="mvcboardVO" class="com.kh.springojdbcTemplate.vo.MvcboardVO"/>

	<!-- MvcboardList 클래스 bean -->
	<bean id="mvcboardList" class="com.kh.springojdbcTemplate.vo.MvcboardList"/>
	
	<!-- Service 클래스 bean을 만든다. -->
	<bean id="contentView" class="com.kh.springojdbcTemplate.service.contentViewService"/>
	
	<bean id="insert" class="com.kh.springojdbcTemplate.service.InsertService"/>
	
	<!-- MvcboardDAO 클래스 bean을 만든다. -->
	<bean id="mvcboardDAO" class="com.kh.springojdbcTemplate.dao.MvcboardDAO"/>
	




</beans>